package org.epmr.utils.collection
{
	public interface IList extends ICollection
	{
		function addItem(index:int,item:*):void;
		
		function addItems(index:int,items:ICollection):void;
		
		function indexOf(item:*):int;
		 
		function lastIndexOf(item:*):int;
		
		function setItem(index:int,item:*):*; 
		
		function getItem(index:int):*;
		
		function removeItem(index:int):*;
		
		function subList(fromIndex:int,toIndex:int):IList;
		
	}
}